@model UserModel
@{
    ViewData["Title"] = "Your profile";
}
<h2>Your profile:</h2>

<div class="row">
    <div class="col-lg-2">
        <h3> Username: </h3>
        <h3> First name: </h3>
        <h3> Last name: </h3>
        <h3> Email: </h3>
        <h3> University: </h3>
        <h3> Status: </h3>
    </div>
    <div class="col-lg-2">
        <h3>@Model.Username</h3>
        <h3>@Model.FirstName</h3>
        <h3>@Model.LastName</h3>
        <h3>@Model.Email</h3>
        <h3>@Model.University</h3>
        <h3>@Model.Status</h3>
    </div>
</div>

